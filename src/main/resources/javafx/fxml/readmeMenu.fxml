<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox fx:id="vbox" prefHeight="400.0" prefWidth="640.0" stylesheets="@../css/signup.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleBack" text="Back" />
            <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane fx:id="anchorPane" maxHeight="370.0" maxWidth="1.7976931348623157E308" prefHeight="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <WebView fx:id="wbView" minHeight="370.0" minWidth="640.0" prefHeight="-1.0" prefWidth="-1.0" />
      </children>
    </AnchorPane>
  </children>
</VBox>
